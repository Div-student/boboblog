.modalMask{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: rgba(0, 0, 0, .1);
  z-index: 999;
}
.modalMask .modalPanel{
  position: absolute;
  width: 100%;
  min-height: 300rpx;
  background-color: #fff;
}
.icon-delete{
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}